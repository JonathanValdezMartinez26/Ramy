	<queryString>
		<![CDATA[Select 
        C.ID_Cita,
        C.ID_Medico,
        concat_ws(' ',M.nombres, M.apellidos ) AS Nombres_med,                            
        C.Fecha_Cita,
        C.Dia_Cita,
        C.Semana_Cita,
        C.Hora_Cita,
        concat_ws(' ',P.nombres, P.apellidos ) AS Nombres_pac,         
        C.Estado
    from
        Cita C
            Inner Join
        Paciente P ON C.ID_Paciente = P.ID_Paciente
            Inner Join
        Medico M ON C.ID_Medico = M.ID_Medico]]>
	</queryString>
