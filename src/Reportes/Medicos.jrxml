	<queryString>
		<![CDATA[Select 
        U.ID_Medico,
        concat_ws(' ',U.nombres, U.apellidos) AS Nombre,
        U.Cedula,
        E.Nombre AS Especialidad,
   CASE U.Estado WHEN U.Estado= true THEN 'Activo' ELSE 'Inactivo' END AS Estado
   from
        Medico U
   Inner Join Especialidad E
   On U.ID_Medico = E.ID_Especialidad]]>
	</queryString>
