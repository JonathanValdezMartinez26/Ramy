	<queryString>
		<![CDATA[Select 
        C.ID_Bitacora,
        C.Nombre_viaje,
        C.Costo_antiguo,
        C.Costo_nuevo,
        C.Fecha_mod,
        C.ID_clienteB ,
        C.Transporte,
        E.Nombre_cliente 

    from
        clientes E
            Inner Join
        bitacora_costos C ON C.ID_clienteB = E.ID_Cliente
        WHERE ID_Cliente=$P{ID_Cliente}
ORDER BY fecha_mod DESC]]>
